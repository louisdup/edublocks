  <block type="style_block"></block>
  
  <block type="stylesheet">
    <value name="attributes">
      <block type="attributes_join">
        <mutation color="#00CA4D" items="2" limit="100" minimum="1" />
        <value name="input_1">
          <block type="rel_attribute">
            <value name="content">
              <shadow type="inline_string">
                <field name="value">stylesheet</field>
              </shadow>
            </value>
          </block>
        </value>
        <value name="input_2">
          <block type="href_attribute">
            <value name="content">
              <shadow type="inline_string">
                <field name="value">URL</field>
              </shadow>
            </value>
          </block>
        </value>
      </block>
    </value>
  </block>

